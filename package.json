{
  "name": "tagger",
  "version": "0.1.0",
  "description": "label entities/relations in text documents",
  "license": "UNLICENSED",
  "author": "Yossi Cohen",
  "keywords": [
    "tagger",
    "react",
    "redux"
  ],
  "main": "index.js",
  "homepage": "http://localhost:9090",
  "repository": {
    "type": "git",
    "url": "https://github.com/yossi-cohen/tagger"
  },
  "scripts": {
    "start": "webpack-dev-server --inline --hot --history-api-fallback --host localhost --port 9090",
    "build-dev": "webpack --config webpack.dev.config.js",
    "build-prod": "webpack -p --config webpack.prod.config.js",
    "test": "mocha --recursive --compilers js:babel-register --require babel-polyfill --require ignore-styles",
    "test-watch": "npm test -- --watch",
    "api": "json-server --watch src/api/db.js --port 3004",
    "backend": "export PYTHONPATH=`pwd` && ~/virtualenv/tagger/bin/python backend/server.py"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-bootstrap": "^0.31.3",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.1",
    "react-router-redux": "^4.0.8",
    "react-tag-input": "^4.7.2",
    "redux": "^3.7.2",
    "redux-form": "^7.1.0",
    "redux-saga": "^0.15.6"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-runtime": "^6.26.0",
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "ignore-styles": "^5.0.1",
    "json-server": "^0.12.0",
    "mocha": "^4.0.1",
    "node-sass": "^4.5.3",
    "react-hot-loader": "^1.3.1",
    "redux-freeze": "^0.1.5",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "whatwg-fetch": "^2.0.3"
  }
}
